VITE_APP_TITLE=3D SaaS Landing Page

# API URLs
VITE_API_BASE_URL=https://api.example.com/v1
VITE_MODEL_API_URL=https://models.example.com/api
VITE_CONTACT_FORM_API_URL=https://contact.example.com/submit

# 3D Model URLs
VITE_DEFAULT_MODEL_URL=/models/laptop.glb
VITE_ENVIRONMENT_MAP_URL=/hdri/skybox.hdr

# Contact Email Recipient
VITE_CONTACT_EMAIL_RECIPIENT=contact@example.com

# Analytics Keys
VITE_GOOGLE_ANALYTICS_KEY=UA-XXXXX-Y
VITE_TRACKJS_TOKEN=1234567890abcdef

# Optional: Feature Flags (example)
VITE_FEATURE_FLAG_NEW_UI=true
VITE_FEATURE_FLAG_ADVANCED_SHADOWS=false

# Public folder (static assets)
VITE_PUBLIC_ASSETS_URL=/public/assets

# Version control
VITE_APP_VERSION=1.0.0

# Image Optimization Service URL
VITE_IMAGE_OPTIMIZATION_URL=https://img.example.com

# Sentry environment configuration
VITE_SENTRY_DSN=

# Local development backend URL (for proxying)
VITE_DEV_API_URL=http://localhost:3000

# Specify WebGL render engine to target
VITE_TARGET_WEBGL_VERSION=2